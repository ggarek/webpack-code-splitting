{
  "name": "webpack-code-splitting",
  "version": "1.0.0",
  "description": "Examples of webpack code spitting",
  "main": "index.js",
  "scripts": {
    "use-bundle-loader:build": "rm -rf dist/use-bundle-loader && webpack --config webpack.config.bundle-loader.js",
    "use-dynamic-import:build": "rm -rf dist/use-dynamic-import && webpack --config webpack.config.dynamic-import.js",
    "build": "npm run use-bundle-loader:build && npm run use-dynamic-import:build"
  },
  "keywords": [
    "webpack",
    "code-splitting",
    "example"
  ],
  "author": "Igor Ovsiannikov <garek@mail.ru>",
  "license": "MIT",
  "devDependencies": {
    "bundle-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.10.0"
  }
}
